{
  "buildCommand": "npm run build",
  "installCommand": "echo 'No dependencies to install'",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/tracingart/(.*)",
      "destination": "/$1"
    }
  ],
  "redirects": [
    {
      "source": "/tracingart",
      "destination": "/",
      "permanent": false
    }
  ]
}
